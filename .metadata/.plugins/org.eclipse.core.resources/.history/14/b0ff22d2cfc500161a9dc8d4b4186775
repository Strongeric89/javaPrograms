package com.lab3;

public class Control
{
	public static void main(String[] args)
	{
		// String first, String second, int staff, double sal, String start)
		Employee eric = new Employee("Eric", "Strong", 1, 2500.00, "January 2008");
		System.out.println(eric);

		HourlyEmployee laura = new HourlyEmployee("Laura", "Bermingham", 2, 0.00, "February 2010", 12, 10);
		System.out.println(laura);
	}// end main

}// end class
