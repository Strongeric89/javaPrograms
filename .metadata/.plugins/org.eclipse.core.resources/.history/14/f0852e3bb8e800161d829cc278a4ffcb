package com.eric.lab1;

public class Animal
{
	private String name;
	private String age;
	
	//default Constructor
	public Animal(){
		
	}//end constructor
	
	//constructor 2 - a name
	public Animal(String name){
		this.name = name;
		
	}
	
	public String toString(){
		return "hello my name is" + this.name;
	}

}//end class
