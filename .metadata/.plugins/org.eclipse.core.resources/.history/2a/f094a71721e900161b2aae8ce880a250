package com.eric.lab1;

public class Person
{
	private String name;
	private int age = 0;
	private boolean isHealthy = false;
	
	//default constructor
	public Person(){
		
	}
	//constructor 2
	public Person(String name)
	{
		this.name = name;

	}// end constructor

	//constructor 3
	public Person(String name, int age, boolean isHealthy){
		
	}//end constructor
	public String getName()
	{
		return name;
	}

	public void setName(String name)
	{
		this.name = name;
	}

}// end class
