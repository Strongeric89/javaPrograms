package com.lab2;

public class Animal
{
	// public attributes
	String name;
	String breed;
	int legs = 0;
	char domesticated;
	String colour;

	// constructor 1
	public Animal(String name)
	{
		this.name = name;
		
		System.out.println("Animal Created.");

	}

	// Constructor 2
	public Animal(String name, String breed, int legs, char dom, String colour)
	{
		this.name = name;
		this.breed = breed;
		this.legs = legs;
		this.domesticated = dom;
		this.colour = colour;
		

	}

}// end class
