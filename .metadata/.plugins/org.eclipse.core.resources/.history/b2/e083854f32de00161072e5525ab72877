package com.io;

import java.io.File;
import java.util.Scanner;

public class ReadApp
{
	public static void main(String[] args)
	{

		File file = new File(test.txt);
		Scanner in = new Scanner(file);

		while (in.hasNextLine())
		{
			String line = in.nextLine();
			System.out.println(line);
		}

	}// end main
}// end class
