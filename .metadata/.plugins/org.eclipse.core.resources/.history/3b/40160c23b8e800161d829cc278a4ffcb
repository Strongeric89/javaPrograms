package com.eric.lab1;

public class ControlApp
{
	public static void main(String[] args)
	{
		
		//instanciating animal objects
		Animal a1 = new Animal(); // this will call upon default constructor cos no perameters passed
		
		Animal a2 = new Animal("Banger"); // this will call upon second constructor. Note the parameter passed 
		
		
	}//end main

}//end class
