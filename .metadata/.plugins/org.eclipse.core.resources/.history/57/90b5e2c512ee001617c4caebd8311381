 */
public class ControlApp
{
	public static void main(String[] args)
	{

		// create objects

		Employee e1 = new Employee("Eric", "Strong", 1, 25000.50, "08/02/2017"); // employee
																					// object
		HourlyEmployee he1 = new HourlyEmployee("Eric", "Strong", 1, 0.00, "09/02/2017", 40, 10.50); // hourlyEmployee
																										// object

		CommissionEmployee ce1 = new CommissionEmployee("Eric", "Strong", 1, 25000.50, "10/02/2017", 550.50); // employee
																												// object
		//print out each object
		System.out.println(e1);
		System.out.println(he1);
		System.out.println(ce1);
		
		
	}// end main Method

}// end ControlApp
