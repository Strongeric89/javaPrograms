package com.lab3;

public class HourlyEmployee extends Employee
{
	// extra attributes
	private double hoursWorked = 0.00;
	private double hourlyRate = 0.00;

	public Employee(double hours, double rate)
	{
		this.hourlyRate=rate;
		this.hoursWorked=hours;

	}

}
