package com.eric;

public class Challenge4 {
	public static void main(String [] args)
	{
		System.out.println("Numbers between 1-50 that are divisable by 5(USING FOR)");
		boolean isRunning = true;
		for(int i=0;i<=50;i++)
		{
			if(i % 5 == 0)
			{
				System.out.print(i+" , ");
			}//end if
		}//end for
		
		while(isRunning)
		{
			
		}//end while
	}//end main
}
